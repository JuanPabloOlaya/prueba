{
  "name": "kumasagati/prueba",
  "description": "This package was created as developer's trial",
  "require": {
    "php": "^7.3"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "Kumasagati\\Prueba\\": "./src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kumasagati\\Prueba\\PruebaServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Juan Pablo",
      "email": "juanpa-1920@hotmail.com"
    }
  ],
  "minimum-stability": "dev",
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan route:scan",
      "php artisan event:scan",
      "php artisan optimize",
      "php artisant migrate"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize",
      "php artisan migrate"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-package-install": [
      "php artisan migrate"
    ]
  }
}
